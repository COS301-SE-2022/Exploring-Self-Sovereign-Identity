{
  "name": "ssivuejsclient",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 3000",
    "host": "vite preview --port 3000 --host",
    "test:unit": "vitest --coverage --environment jsdom",
    "test:e2e": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress open'",
    "test:e2e:ci": "start-server-and-test preview http://127.0.0.1:5050/ 'cypress run'",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "build:prerelease": "vite build --mode prerelease",
    "deps": "yarn upgrade-interactive --latest",
    "lint:es": "eslint \"src/**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:prettier": "prettier --write \"src/**/*.{md,json,html}\"",
    "lint:style": "stylelint \"src/**/*.{css,scss,vue}\"",
    "lint:ts": "tsc --noEmit --skipLibCheck",
    "test": "jest"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.5",
    "@material/card": "^14.0.0",
    "@material/mwc-button": "^0.26.1",
    "@passageidentity/passage-auth": "^0.1.0-beta.28",
    "@passageidentity/passage-elements": "^0.1.0-beta.43",
    "@vicons/carbon": "^0.12.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "axios": "^0.27.2",
    "canvas-confetti": "^1.4.0",
    "clipboard": "^2.0.8",
    "codecov": "^3.8.3",
    "element-plus": "^2.2.4",
    "html2canvas": "^1.3.2",
    "jszip": "^3.10.0",
    "object-hash": "^3.0.0",
    "perfect-scrollbar": "^1.5.2",
    "pinia": "^2.0.13",
    "vue": "^3.2.33",
    "vue-i18n": "^9.2.0-beta.9",
    "vue-router": "^4.0.15",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@cypress/vue": "^3.1.2",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/canvas-confetti": "^1.4.2",
    "@types/jest": "^27.0.2",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^16.11.27",
    "@types/object-hash": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "@vicons/ionicons5": "^0.12.0",
    "@vicons/material": "^0.12.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^2.0.0-rc.20",
    "@vue/tsconfig": "^0.1.3",
    "babel-jest": "^27.2.5",
    "c8": "^7.11.3",
    "cypress": "^10.0.3",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "jest": "^27.2.5",
    "jsdom": "^19.0.0",
    "lint-staged": "^12.3.3",
    "naive-ui": "^2.32.1",
    "prettier": "2.7.1",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.52.1",
    "sass-loader": "^13.0.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-rational-order": "^0.0.4",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "stylelint-scss": "^4.1.0",
    "typescript": "~4.6.3",
    "unplugin-auto-import": "^0.8.8",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-components": "^0.19.9",
    "vfonts": "^0.0.3",
    "vite": "^2.9.5",
    "vitest": "^0.9.4",
    "vue-tsc": "^0.34.7",
    "webpack-dev-server": "^4.9.0"
  }
}
